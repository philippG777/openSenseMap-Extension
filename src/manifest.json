{
  "manifest_version": 2,
  "name": "openSenseMap Extension",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["https://opensensemap.org/explore/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "logo.min.svg",
    "default_popup": "popup.html",
    "default_title": "View values from a senseBox!"
  }
}
